{
"manifest_version": 2,
    "name": "Abuse Detection",
    "description": "Detects and supresses profanities words",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
